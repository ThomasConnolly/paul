<h1>Members of St. Paul's</h1>


<% @members.each do |m| %>
  <p><strong>
      <%= m.username.titleize %>
    </strong>,&nbsp;
    <%= m.membership_id %>, &nbsp;<%= m.email %>, &nbsp;<%= m.birthday %> <small>
            <%= link_to 'Edit' , edit_member_path(m) %>
              <%= link_to ' | Delete' , member_path(m), method: :delete, data: { confirm: 'Are you sure?' } %>
          </small>
  </p>
  <% end %>

    <h2> Members in the diaspora </h2>
    <% @diaspora.each do |d| %>
      <p>
        <%=d.first_name %>
          <%=d.last_name %>
            <%= d.email %>
      </p>
      <% end %>


<%= bootstrap_form_for(@book) do |f| %>
  <%= f.text_field :author %>
  <%= f.text_field :title %>
  <%= f.text_field :subject %>
  <%= f.text_field :isbn %>
  <%= f.text_field :dewey %>
  <%= f.text_area :description %>
  <%= f.url_field :url %>
  <%= f.submit 'Submit', class: "btn btn-primary" %>
<% end %>

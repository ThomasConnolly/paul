<%= bootstrap_form_for(@story_assignment) do |f| %>

  <div class="form-inputs">
    <%= f. :story_idea %>
      <%= f.collection_select :assigned_to, User.communicator.username %>
        <%= f.input :status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>
  <% end %>
<h1>Stripe Reports</h1>
<div class="container"> 
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Amount</th>
      <th>Date</th>
      <th>Stripe fee</th>
      <th>Net</th>
S
    </tr>
  </thead>
    <tbody>
      <tr><% @stripe_reports.each do |sr| %>
        <td><%= sr.donor_name %></td>
        <td><%= number_to_currency(sr.amount/100.0) %></td>
        <td><%= sr.created_at.strftime("%b %-d, %Y") %>  
          <% end %>  
        </td>
        <td><%= number_to_currency(sr.stripe_fee/100.0) %></td>
        <td><%= number_to_currency(sr.net/100.0) %></td>

      </tr>
    </tbody>
    <% end %>
  </table>
</div>
<%= simple_form_for @candidate do |f| %>
    <%= f.input :last_name %>
    <%= f.input :full_name %>
    <%= f.input :url %>
    <%= f.input :links_narrative %>
    <%= f.input :link1 %>
    <%= f.input :link2 %>
    <%= f.input :link3 %>
    <%= f.input :link4 %>
    <%= f.input :photo_link %>

    <%= f.button :submit, class: 'btn btn-primary' %>
<% end %>

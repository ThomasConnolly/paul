<%= simple_form_for(@sponsorship) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :amount %>
    <%= f.input :plan %>
    <%= f.input :customer_id %>
    <%= f.input :card %>
    <%= f.input :last_4 %>
    <%= f.input :expiraton_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
